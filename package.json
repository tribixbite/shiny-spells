{
  "name": "elysiajs-drizzleORM-lucia-auth",
  "version": "1.0.50",
  "scripts": {
    "ex": "bun build --compile --sourcemap ./src/index.ts --outfile build/app && ./build/app",
    "test": "bun test --coverage",
    "dev": "bun run --watch src/index.ts",
    "b2": "bun build ./src/index.ts ./src/routes/**/**/* --outfile build/app",
    "bin": "bun build --minify --sourcemap ./src/index.ts ./src/routes/**/**/* --outdir build --target bun",
    "buildex": "bun build --compile --minify --sourcemap ./src/index.ts ./src/routes/**/**/* --outfile build/app",
    "start": "bun ./src/index.ts",
    "tsc": "tsc && bun ./src/index.ts",
    "biome:lint": "bunx biome lint --write src",
    "biome:format": "bunx biome format --write src",
    "check": "bunx biome check --write src",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:rollback": "drizzle-kit rollback",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:introspect": "drizzle-kit introspect",
    "ragify": "bun run ./scripts/ragify/ragify.ts"
  },
  "dependencies": {
    "@biomejs/biome": "^1.8.3",
    "@elysiajs/cors": "^1.0.2",
    "@elysiajs/swagger": "^1.0.5",
    "@kinobi-so/nodes-from-anchor": "^0.20.9",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@metaplex-foundation/kinobi": "^0.18.8-alpha.0",
    "@t3-oss/env-core": "^0.10.1",
    "drizzle-orm": "^0.31.2",
    "elysia": "latest",
    "elysia-autoroutes": "^0.5.0",
    "elysia-rate-limit": "^4.1.0",
    "kinobi": "^0.21.0",
    "lucia": "^3.2.0",
    "oslo": "^1.2.1",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.1",
    "postgres": "^3.4.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "drizzle-kit": "^0.22.7",
    "lefthook": "^1.7.6",
    "pg": "^8.12.0",
    "simple-git": "^3.25.0",
    "typescript": "^5.5.4"
  },
  "module": "index.js",
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
